        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "def delta(n):\n",
        "  r = np.zeros(n.size)\n",
        "  for i in range(n.size):\n",
        "    if n[i] == 0:\n",
        "      r[i] = 1\n",
        "  return r\n",
        "\n",
        "n = np.arange(-5, 6)\n",
        "y = 2*delta(n+2) - delta(n-4)\n",
        "plt.stem(n, y)\n",
        "plt.show()"
      ]
